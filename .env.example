# Example environment variables for Bajon Consulting app
# Copy this file to .env and fill values (do NOT commit .env)

# Supabase
SUPABASE_URL=https://your-supabase-project.supabase.co
SUPABASE_ANON_KEY=your_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Backend (Spring Boot)
SPRING_DATASOURCE_URL=jdbc:postgresql://db:5432/bajon_audit_db
SPRING_DATASOURCE_USERNAME=bajon
SPRING_DATASOURCE_PASSWORD=change_me

# Frontend (Vite) - VITE_ prefix required by Vite to expose to client
VITE_API_URL=http://localhost:8080
VITE_SUPABASE_URL=${SUPABASE_URL}
VITE_SUPABASE_ANON_KEY=${SUPABASE_ANON_KEY}
